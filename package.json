{
  "name": "expression-exchange",
  "version": "1.2.0-dev.2",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/expression-exchange.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "EXPRESSION_EXCHANGE",
    "phetLibs": [
      "twixt",
      "vegas"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "simFeatures": {
      "supportsSound": true
    },
    "simulation": true,
    "supportsOutputJS": true,
    "published": true,
    "screenNameKeys": [
      "EXPRESSION_EXCHANGE/basics",
      "EXPRESSION_EXCHANGE/explore",
      "EXPRESSION_EXCHANGE/negatives",
      "EXPRESSION_EXCHANGE/game"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "parserOptions": {
          "project": [
            "../chipper/tsconfig/eslint-all/tsconfig.json"
          ]
        }
      }
    ]
  }
}